{
  "templates": {
    "title_template": {
      "type": "text",
      "$text": "title_link",
      "font_size": 20,
      "line_height": 24,
      "text_color": "#1A0DAB",
      "max_lines": 3,
      "action": {
        "$url": "title_link_url",
        "log_id": "title_link_url"
      },
      "margins": {
        "left": 16,
        "right": 16
      }
    },
    "images_gallery_template": {
      "type": "gallery",
      "width": {
        "type": "match_parent"
      },
      "paddings": {
        "top": 8,
        "left": 16,
        "right": 16
      },
      "item_spacing": 4,
      "$items": "items"
    },
    "result_image_template": {
      "type": "image",
      "$image_url": "image_url",
      "width": {
        "type": "fixed",
        "value": 70
      },
      "height": {
        "type": "fixed",
        "value": 70
      },
      "action": {
        "$url": "image_click_url",
        "log_id": "image_click"
      },
      "border": {
        "corner_radius": 8
      },
      "placeholder_color": "#00ffffff"
    },
    "yandex_images_template": {
      "type": "container",
      "orientation": "horizontal",
      "items": [
        {
          "type": "image",
          "$image_url": "site_favicon",
          "width": {
            "type": "fixed",
            "value": 16
          },
          "height": {
            "type": "match_parent"
          },
          "placeholder_color": "#00ffffff"
        },
        {
          "type": "text",
          "text": "Яндекс.Картинки",
          "font_size": 15,
          "line_height": 20,
          "text_color": "#007700",
          "max_lines": 1,
          "width": {
            "type": "wrap_content"
          },
          "margins": {
            "left": 4
          }
        }
      ],
      "margins": {
        "top": 1,
        "left": 16,
        "right": 16
      }
    },
    "card_template": {
      "type": "container",
      "orientation": "vertical",
      "height": {
        "type": "wrap_content"
      },
      "background": [
        {
          "type": "solid",
          "color": "#fff"
        }
      ],
      "border": {
        "corner_radius": 12,
        "stroke": {
          "color": "#E7E9EC",
          "width": 1
        }
      },
      "paddings": {
        "top": 16,
        "bottom": 16
      },
      "margins": {
        "top": 8,
        "bottom": 8
      },
      "$items": "items"
    }
  },
